{
  "name": "cybermcp",
  "version": "0.2.0",
  "description": "Cybersecurity API Testing with Model Context Protocol",
  "type": "module",
  "main": "dist/index.js",
  "bin": {
    "cybermcp": "./dist/index.js"
  },
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "ts-node --esm src/index.ts",
