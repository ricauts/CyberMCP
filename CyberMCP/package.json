    "axios": "^1.7.7",
    "zod": "^3.23.8",
    "jwt-decode": "^4.0.0",
    "cheerio": "^1.0.0",
    "crypto": "^1.0.1"
  },
  "devDependencies": {
    "@types/node": "^20.12.7",
    "typescript": "^5.4.5",
    "ts-node": "^10.9.2",
    "jest": "^29.7.0",
    "ts-jest": "^29.2.4",
    "@modelcontextprotocol/inspector": "^0.1.0"
  },
  "files": [
    "dist",
    "README.md"
